<%= button_to "Sign Out", destroy_user_session_path, method: :delete %>
<h1> <%= @doctor.name %>: Appointments </h1>
<%= link_to "Add Enrollment", new_doctor_appointment_path(@doctor) %>
<hr />

<h2> Doctor </h2>
<ul>
  <% @docapps.each do |docapp| %>
    <li>
      <%= docapp.user.full_name %>
      <br />
      <%= link_to 'Remove', doctor_appointment_path(@doctor, doctor_appt), method: :delete %>
      </li>
      <% end %>
    </ul>

<h2> Patient </h2>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.user.full_name %>
      <br />
      <%= link_to 'Remove', doctor_appointment_path(@doctor, user), method: :delete %>
      </li>
    <% end %>
  </ul>